ber\">Methods {{ counterButton }}</button>\n<button @click=\"countComputed\">Computed {{ numberComputed }}</button>\n<P>----------class dan style-----------</P>\n<ul>\n  <li class=\"\" :class=\"{active: isActive,\n                        fs40px: isActive}\">\n    test\n  </li>\n</ul>\n<button @click=\"ubahWarna\">Ubah</button>\n\n</template>\n\n<script>\nexport default {\n  data(){\n    return{\n      kondisi:'show',\n      counterButton: 0,\n      numberComputed: 0,\n      show: false,\n      count : 1,\n      nama: 'dudan',\n      number: 1,\n      kelas: '<h1>kelas 11</h1>',\n      nonaktif: false,\n      property: {\n        id: 1,\n        class: 'color'\n      },\n      typeInput: 'password',\n      isActive:true\n    }\n  },\n  methods: {\n  counterNumber(){\n    this.counterButton += 1;\n  },\n  showPassword() {\n    if(this.typeInput == 'password') {\n      this.typeInput = 'text';\n      this.kondisi = 'hide';\n    }else{\n      this.typeInput = 'password';\n    }\n  }\n  },\n  ubahWarna(){\n    if(this.isActive == true){\n      this.isActive=false;\n    }else{\n      this.isActive=true;\n    }\n  },\n  computed:{\n    numberComputed (){\n      this.countComputedComputed += 6;\n    }\n  }\n}\n</script>\n\n<style>\n.color{\ncolor: red;\n}\n.fs40px{\n  font-size: 20px;\n}\n.active{\n  color: brown;\n}\n</style>"],"mappings":"AA0CA,eAAe;EACbA,IAAIA,CAAA,EAAE;IACJ,OAAM;MACJC,OAAO,EAAC,MAAM;MACdC,aAAa,EAAE,CAAC;MAChBC,cAAc,EAAE,CAAC;MACjBC,IAAI,EAAE,KAAK;MACXC,KAAI,EAAI,CAAC;MACTC,IAAI,EAAE,OAAO;MACbC,MAAM,EAAE,CAAC;MACTC,KAAK,EAAE,mBAAmB;MAC1BC,QAAQ,EAAE,KAAK;MACfC,QAAQ,EAAE;QACRC,EAAE,EAAE,CAAC;QACLC,KAAK,EAAE;MACT,CAAC;MACDC,SAAS,EAAE,UAAU;MACrBC,QAAQ,EAAC;IACX;EACF,CAAC;EACDC,OAAO,EAAE;IACTC,aAAaA,CAAA,EAAE;MACb,IAAI,CAACd,aAAY,IAAK,CAAC;IACzB,CAAC;IACDe,YAAYA,CAAA,EAAG;MACb,IAAG,IAAI,CAACJ,SAAQ,IAAK,UAAU,EAAE;QAC/B,IAAI,CAACA,SAAQ,GAAI,MAAM;QACvB,IAAI,CAACZ,OAAM,GAAI,MAAM;MACvB,CAAC,MAAI;QACH,IAAI,CAACY,SAAQ,GAAI,UAAU;MAC7B;IACF;EACA,CAAC;EACDK,SAASA,CAAA,